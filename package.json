{
  "name": "smart-skills-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "^2.32.1",
    "@babel/eslint-parser": "^7.19.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/styles": "^5.11.0",
    "@mui/x-data-grid": "^5.17.14",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^12.1.10",
    "clsx": "^1.2.1",
    "d3": "^7.7.0",
    "formik": "^2.2.9",
    "formik-mui": "^5.0.0-alpha.0",
    "http-proxy-middleware": "^2.0.6",
    "js-cookies": "^1.0.4",
    "lodash": "^4.17.21",
    "notistack": "^2.0.8",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-helmet-async": "^1.3.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.2",
    "react-tagcloud": "^2.1.1",
    "redux-persist": "^6.0.0",
    "web-vitals": "^3.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "serve build",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.jsx,.ts,.tsx",
    "test": "react-scripts test --watchAll=false --transformIgnorePatterns \"./node_modules/(?!d3|d3-array)/\"",
    "test:watch": "react-scripts test  --watch",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "cypress:open": "cypress run",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@azure/msal-react": "^1.5.1",
    "cypress": "^12.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "formik-mui-lab": "^1.0.0",
    "husky": "^8.0.2",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "serve": "^14.1.2"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "store/index.js$",
      "constants",
      "styles.js",
      "reportWebVitals.js",
      "setupProxy.js",
      "src/index.js",
      "mocks"
    ],
    "moduleNameMapper": {
      "^constants/(.*)$": "<rootDir>/src/constants/$1",
      "^assets/(.*)$": "<rootDir>/src/assets/$1",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^pages/(.*)$": "<rootDir>/src/pages/$1",
      "^theme/(.*)$": "<rootDir>/src/theme/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^routers/(.*)$": "<rootDir>/src/routers/$1",
      "^containers/(.*)$": "<rootDir>/src/containers/$1",
      "^store/(.*)$": "<rootDir>/src/store/$1",
      "^slices/(.*)$": "<rootDir>/src/slices/$1",
      "^api/(.*)$": "<rootDir>/src/api/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  }
}
